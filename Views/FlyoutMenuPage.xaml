<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:Referidos.Views"
            x:Class="Referidos.Views.FlyoutMenuPage">
    <FlyoutPage.Flyout>
        <NavigationPage>
            <x:Arguments>
                <local:MenuPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:PrincipalPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>
 